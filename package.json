{
  "name": "TailHub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "align:check": "rnx-align-deps --requirements react-native@0.81",
    "align:fix": "rnx-align-deps --requirements react-native@0.81 --write && yarn install",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:all": "eslint --max-warnings=0 --ignore-path .eslintignore --ext ts --ext tsx --ext js --ext jsx .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --check",
      "eslint --max-warnings=0"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.81.4",
    "react": "19.1.0",
    "react-native": "^0.81.0",
    "react-native-safe-area-context": "^5.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^20.0.0",
    "@react-native-community/cli-platform-android": "^20.0.0",
    "@react-native-community/cli-platform-ios": "^20.0.0",
    "@react-native/babel-preset": "^0.81.0",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "^0.81.0",
    "@react-native/typescript-config": "0.81.4",
    "@rnx-kit/align-deps": "^3.2.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "index.js",
  "author": "NovigradWitcher",
  "license": "MIT",
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.81"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "community/cli",
        "community/cli-android",
        "community/cli-ios",
        "core",
        "core-android",
        "core-ios",
        "core/metro-config",
        "jest",
        "react",
        "react-test-renderer",
        "safe-area"
      ]
    }
  }
}
